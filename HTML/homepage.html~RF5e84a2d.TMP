<!-- Feedback -->
<!-- Button trigger modal -->
<button type="button" class="btn btn-primary tempBtn1" data-toggle="modal" data-target="#exampleModalCenter" style="">
  Launch feedback
</button>

<!-- Modal -->
<div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document" style="margin-top:20px;">
    <div class="modal-content">
      <div class="modal-header">
        <h3 class="modal-title" id="exampleModalLongTitle" style="display: contents;">Feedback</h3>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <form>
          <div class="modal-body">
            <div class="fbSelect">
                <p>We would like you feedback to improve our website</p>
                <p>What is your opinion about our website?</p>
                <div class="nav nav-tabs">
                    <ul class="feedbackIcon list-inline">
                        <li><a data-toggle="tab" href="#imgFB1" class="imgFB1"><img class="feedbackIco imgFB1" src="assets/fill-5.svg" /></a></li>
                        <li><a data-toggle="tab" href="#imgFB2" class="imgFB2"><img class="feedbackIco" id="imgFB2" src="assets/fill-8.svg" /></a></li>
                        <li><a data-toggle="tab" href="#imgFB3" class="imgFB3"><img class="feedbackIco" id="imgFB3" src="assets/fill-1.svg" /></a></li>
                        <li><a data-toggle="tab" href="#imgFB4" class="imgFB4"><img class="feedbackIco" id="imgFB4" src="assets/fill-3.svg" /></a></li>
                        <li><a data-toggle="tab" href="#imgFB5" class="imgFB5"><img class="feedbackIco" id="imgFB5" src="assets/fill-10.svg" /></a></li>
                    </ul>
                    <input type="radio" value="1" name="opinion" id="f1" />
                    <input type="radio" value="2" name="opinion" id="f2" />
                    <input type="radio" value="3" name="opinion" id="f3" />
                    <input type="radio" value="4" name="opinion" id="f4" />
                    <input type="radio" value="4" name="opinion" id="f5" />
                </div>
            </div>
            <div class="divider"></div>        
            <div class="fbCat">
                <p>Please select your feedback catefory below</p>
                <div class="nav nav-tabs">
                    <ul class="fbCategory list-inline">
                        <li><a data-toggle="tab" href="#c1">Compliment</a></li>
                        <li><a data-toggle="tab" href="#c2">Something isn't quite right</a></li>
                        <li><a data-toggle="tab" href="#c3">Suggestion</a></li>
                    </ul>
                </div>
            </div>
            <div class="clearfix"></div>
            <div class="divider"></div>
            <div class="fbContent">
                <p>Leave your feedback below</p>
                <textarea name="txtFeedback"></textarea>
            </div>
          </div>
          <div class="modal-footer">
            <button type="button" onclick="test()" class="btn btn-primary">Save changes</button>
          </div>
      </form>
    </div>
  </div>
</div>
<!-- End Feedback -->



<!-- Login -->
<button type="button" class="btn btn-primary tempBtn4" data-toggle="modal" data-target="#exampleModal4">Login</button>

<div class="modal fade" id="exampleModal4" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document" style="margin-top:20px; width:280px;">
    <div class="modal-content">
      <div class="modal-header">
        <h3 class="modal-title" id="exampleModalLabel" style="display: contents;">Login</h3>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <form>
          <div class="form-group">
            <label for="username" class="col-form-label">Username:</label>
            <input type="text" class="form-control" id="username" />
          </div>
          <div class="form-group">
            <label for="userPass" class="col-form-label">Password:</label>
            <input type="password" class="form-control" id="userPass" />
          </div>
        </form>
        <a href="#">Forgot password?</a>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
        <button type="button" class="btn btn-primary">Login</button>
      </div>
    </div>
  </div>
</div>
<!-- End login -->

